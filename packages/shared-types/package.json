{
  "name": "@laughtrack/shared-types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for Laughtrack domain models",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "echo \"No tests\" && exit 0",
    "lint": "echo \"No linting\" && exit 0",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
